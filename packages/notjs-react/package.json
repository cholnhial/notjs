{
  "name": "notjs-react",
  "version": "0.5.1",
  "description": "A React component for interactive code execution for C,C++,Java, Go and Rust",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./dist/notjs-react.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "build:prod": "vite build",
    "prepublishOnly": "npm run build:prod"
  },
  "keywords": [
    "react",
    "code-execution",
    "terminal",
    "websocket",
    "monaco-editor",
    "xterm"
  ],
  "author": "Chol Nhial <chol.nhial.chol@gmail.com> (https://chol.dev)",
  "license": "MIT",
  "peerDependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0-rc.0",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@xterm/xterm": "^5.5.0",
    "lucide-react": "^0.469.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.3.4",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.6.2",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cholnhial/notjs.git",
    "directory": "packages/notjs-react"
  }
}
